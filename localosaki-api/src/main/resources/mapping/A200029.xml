<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

	<!-- *** デマンド日報(取得) テンプレート *** -->
	<!-- ベース部分 -->
	<template name="base">
		<field name="productCd" length="2" keepPadding="true" />
		<field name="address" length="2" keepPadding="true" />
		<field name="command" length="3" default="DB0" keepPadding="true" />
		<field name="reportHist" length="2" padding="0" justify="right" />
	</template>
	<template name="base.res">
		<field name="productCd" length="2" keepPadding="true" />
		<field name="address" length="2" keepPadding="true" />
		<field name="command" length="5" keepPadding="true" />
	</template>

	<!-- 共通部分１（受信用） -->
	<template name="commonResponse">
		<field name="measureDayTime" length="10" keepPadding="true" />
		<field name="aggregateHour" length="6" keepPadding="true" />
		<field name="occursPointNum" length="2" setter="setOccursPointNum" />
		<segment name="mesuareTimeList" collection="list" class="map" occurs="24">
			<field name="firstHalfDemand" length="4" keepPadding="true" />
			<field name="secondHalfDemand" length="4" keepPadding="true" />
		</segment>
		<field name="dayMaxDemand" length="4" keepPadding="true" />
		<field name="timeOccurDayMaxDemand" length="10" keepPadding="true" />
		<field name="monthMaxDemand" length="4" keepPadding="true" />
		<field name="timeOccurMonthMaxDemand" length="10" keepPadding="true" />
		<field name="yearMaxDemand" length="4" keepPadding="true" />
		<field name="timeOccurYearMaxDemand" length="10" keepPadding="true" />
		<segment name="measurePointList" collection="list" class="map" occursRef="occursPointNum">
			<segment name="mesuareTimeList" collection="list" class="map" occurs="24">
				<field name="firstHalfDemand" length="4" keepPadding="true" />
				<field name="secondHalfDemand" length="4" keepPadding="true" />
			</segment>
		</segment>
	</template>

	<!-- 「00」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="00.req" format="fixedlength">
		<record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200029Param" />
	</stream>
	<!-- 受信文 -->
	<stream name="00.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200029Param">
			<include template="commonResponse" />
		</record>
	</stream>

	<!-- 「03」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="03.req" format="fixedlength">
		<record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200029Param" />
	</stream>

	<!-- 受信文 -->
	<stream name="03.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200029Param">
			<include template="commonResponse" />
		</record>
	</stream>

	<!-- 「06」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="06.req" format="fixedlength">
		<record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200029Param">
		</record>
	</stream>

	<!-- 受信文 -->
	<stream name="06.res" format="fixedlength">
		<record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200029Param">
			<include template="commonResponse" />
		</record>
	</stream>

	<!-- 「12」非対応 -->
	<!-- 送信文 -->
	<stream name="12.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="12.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「21」非対応 -->
	<!-- 送信文 -->
	<stream name="21.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="21.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「22」非対応 -->
	<!-- 送信文 -->
	<stream name="22.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="22.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

</beanio>