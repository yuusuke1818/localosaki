<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

  <!-- *** AielMasterスケジュール(設定) テンプレート *** -->
  <!-- ベース部分 -->
  <template name="base">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" default="XM001" keepPadding="true" />
    <field name="dateTime" length="10" keepPadding="true" />
  </template>

  <template name="base22">
    <include template="base" />
    <field name="compressionDataLength" length="6" default="000000" keepPadding="true" />
  </template>

  <template name="base.res">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" keepPadding="true" />
  </template>

  <template name="base22.res">
    <include template="base.res" />
    <field name="dateTime" length="10" keepPadding="true" />
    <field name="compressionDataLength" length="6" keepPadding="true" />
  </template>

  <!-- 共通部分１（送信用） -->
  <template name="commonRequest_1">

    <segment name="weekList" collection="list" class="map" occurs="8">
      <segment name="businessTimeZoneList" collection="list" class="map" occurs="2">
        <field name="openHourMinute" length="4" keepPadding="true" />
        <field name="closeHourMinute" length="4" keepPadding="true" />
      </segment>
      <segment name="ecoTimeZoneList" collection="list" class="map" occurs="2">
        <field name="startHourMinute" length="4" keepPadding="true" />
        <field name="endHourMinute" length="4" keepPadding="true" />
      </segment>
    </segment>

    <segment name="specifiedDateList" collection="list" class="map" occurs="10">
      <field name="monthDay" length="4" keepPadding="true" />
      <segment name="businessTimeZoneList" collection="list" class="map" occurs="2">
        <field name="openHourMinute" length="4" keepPadding="true" />
        <field name="closeHourMinute" length="4" keepPadding="true" />
      </segment>
      <segment name="ecoTimeZoneList" collection="list" class="map" occurs="2">
        <field name="startHourMinute" length="4" keepPadding="true" />
        <field name="endHourMinute" length="4" keepPadding="true" />
      </segment>
    </segment>

  </template>

  <!-- 「00」非対応 -->
  <!-- 送信文 -->
  <stream name="00.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="00.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「03」非対応 -->
  <!-- 送信文 -->
  <stream name="03.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="03.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「06」非対応 -->
  <!-- 送信文 -->
  <stream name="06.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="06.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「12」非対応 -->
  <!-- 送信文 -->
  <stream name="12.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="12.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「21」非対応 -->
  <!-- 送信文 -->
  <stream name="21.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="21.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

    <!-- 「22」製品に対する設定 -->
    <!-- 送信文 -->
    <stream name="22.req" format="fixedlength">
        <record name="response" template="base22" class="jp.co.osaki.osol.mng.param.A210002Param">
            <include template="commonRequest_1" />
        </record>
    </stream>

    <!-- 受信文 -->
    <stream name="22.res" format="fixedlength">
        <record name="response" template="base22.res" class="jp.co.osaki.osol.mng.param.A210002Param">
        </record>
    </stream>

</beanio>