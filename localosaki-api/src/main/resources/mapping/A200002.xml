<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

	<!-- *** 温度制御(取得) テンプレート *** -->
	<!-- ベース部分 -->
	<template name="base">
		<field name="productCd" length="2" keepPadding="true" />
		<field name="address" length="2" keepPadding="true" />
		<field name="command" length="1" default="V" keepPadding="true" />
		<field name="settingChangeHist" length="1" default="0" keepPadding="true" />
		<field name="command" length="3" default="011" keepPadding="true" />
	</template>

	<template name="base.res">
		<field name="productCd" length="2" keepPadding="true" />
		<field name="address" length="2" keepPadding="true" />
		<field name="command" length="5" keepPadding="true" />
	</template>

	<!-- 共通部分１（受信用） -->
	<template name="commonResponse">
		<field name="settingDate" length="11" keepPadding="true" />
		<field name="ctrlTH" length="1" keepPadding="true" />
		<field name="ctrlTermsTH" length="1" keepPadding="true" />
		<field name="terminalEquipaddressCW" length="2" keepPadding="true" />
		<field name="demandGangCtrlTermsEx" length="1" keepPadding="true" />
		<field name="demandGangCtrlTermsCa" length="1" keepPadding="true" />
		<field name="demandGangCtrlTermsBl" length="1" keepPadding="true" />
		<field name="demandGangCtrlTermsLi" length="1" keepPadding="true" />
		<field name="demandGangCtrlTermsHi" length="1" keepPadding="true" />
		<segment name="ctrlTimeZoneTHList" collection="list" class="map" occurs="6">
			<field name="startHour" length="2" keepPadding="true" />
			<field name="startMinute" length="2" keepPadding="true" />
			<field name="endHour" length="2" keepPadding="true" />
			<field name="endMinute" length="2" keepPadding="true" />
		</segment>
	</template>


	<!-- 「00」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="00.req" format="fixedlength">
		<record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200002Param" />
	</stream>

	<!-- 受信文 -->
	<stream name="00.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200002Param">
			<include template="commonResponse" />
			<segment name="settingCtrlPortList" collection="list" class="map" occurs="16">
				<field name="ctrlPermissionTH" length="1" keepPadding="true" />
				<field name="demandGangCtrlPermission" length="1" keepPadding="true" />
				<field name="sensoraddressTH" length="2" keepPadding="true" />
				<field name="temperatureMax" length="2" keepPadding="true" />
				<field name="temperatureMin" length="2" keepPadding="true" />
				<field name="humidityMax" length="2" keepPadding="true" />
				<field name="humidityMin" length="2" keepPadding="true" />
				<field name="temperatureReturnWidth" length="3" keepPadding="true" />
				<field name="humidityReturnWidth" length="3" keepPadding="true" />
				<field name="loadBlockHoldTime" length="2" keepPadding="true" />
				<field name="loadReturnHoldTime" length="2" keepPadding="true" />
			</segment>
		</record>
	</stream>

	<!-- 「03」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="03.req" format="fixedlength">
		<record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200002Param" />
	</stream>

	<!-- 受信文 -->
	<stream name="03.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200002Param">
			<include template="commonResponse" />
			<segment name="settingCtrlPortList" collection="list" class="map" occurs="16">
				<field name="ctrlPermissionTH" length="1" keepPadding="true" />
				<field name="demandGangCtrlPermission" length="1" keepPadding="true" />
				<field name="sensoraddressTH" length="2" keepPadding="true" />
				<field name="temperatureMax" length="2" keepPadding="true" />
				<field name="temperatureMin" length="2" keepPadding="true" />
				<field name="humidityMax" length="2" keepPadding="true" />
				<field name="humidityMin" length="2" keepPadding="true" />
				<field name="temperatureReturnWidth" length="3" keepPadding="true" />
				<field name="humidityReturnWidth" length="3" keepPadding="true" />
				<field name="loadBlockHoldTime" length="2" keepPadding="true" />
				<field name="loadReturnHoldTime" length="2" keepPadding="true" />
			</segment>
		</record>
	</stream>

	<!-- 「06」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="06.req" format="fixedlength">
		<record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200002Param" />
	</stream>

	<!-- 受信文 -->
	<stream name="06.res" format="fixedlength">
		<record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200002Param">
			<include template="commonResponse" />
			<segment name="settingCtrlPortList" collection="list" class="map" occurs="16">
				<field name="ctrlPermissionTH" length="1" keepPadding="true" />
				<field name="switchChoiceCW" length="1" keepPadding="true" />
				<field name="sensoraddressTH" length="2" keepPadding="true" />
				<field name="temperatureMax" length="2" keepPadding="true" />
				<field name="temperatureMin" length="2" keepPadding="true" />
				<field name="humidityMax" length="2" keepPadding="true" />
				<field name="humidityMin" length="2" keepPadding="true" />
				<field name="temperatureReturnWidth" length="3" keepPadding="true" />
				<field name="humidityReturnWidth" length="3" keepPadding="true" />
				<field name="loadBlockHoldTime" length="2" keepPadding="true" />
				<field name="loadReturnHoldTime" length="2" keepPadding="true" />
			</segment>
		</record>
	</stream>

	<!-- 「12」非対応 -->
	<!-- 送信文 -->
	<stream name="12.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="12.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「21」非対応 -->
	<!-- 送信文 -->
	<stream name="21.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="21.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「22」非対応 -->
	<!-- 送信文 -->
	<stream name="22.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="22.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

</beanio>