<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

	<!-- *** 無線温度ロガー履歴(取得) テンプレート *** -->
	<!-- ベース部分 -->
	<template name="base">
		<field name="productCd" length="2" keepPadding="true" />
		<field name="address" length="2" keepPadding="true" />
		<field name="command" length="5" default="QC000" keepPadding="true" />
	</template>

	<template name="base.res">
		<field name="productCd" length="2" keepPadding="true" />
		<field name="address" length="2" keepPadding="true" />
		<field name="command" length="5" keepPadding="true" />
	</template>

	<!-- 共通部分１（受信用） -->
	<template name="commonResponse">
		<segment name="temperatureLoggerHistList" collection="list" class="map" occurs="200">
			<field name="recordDayTimeSeconds" length="12" keepPadding="true" />
			<field name="addressPd" length="2" keepPadding="true" />
			<field name="addressCd" length="1" keepPadding="true" />
			<field name="elapsedTime" length="4" keepPadding="true" />
			<field name="receiverSensitivity" length="1" keepPadding="true" />
			<field name="batteryState" length="1" keepPadding="true" />
		</segment>
	</template>

	<!-- 「00」(非対応) -->
	<!-- 送信文 -->
	<stream name="00.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="00.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「03」(非対応) -->
	<!-- 送信文 -->
	<stream name="03.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="03.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「06」(非対応) -->
	<!-- 送信文 -->
	<stream name="06.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="06.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「12」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="12.req" format="fixedlength">
		<record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200037Param" />
	</stream>

	<!-- 受信文 -->
	<stream name="12.res" format="fixedlength">
		<record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200037Param">
			<include template="commonResponse" />
		</record>
	</stream>

	<!-- 「21」非対応 -->
	<!-- 送信文 -->
	<stream name="21.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="21.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「22」非対応 -->
	<!-- 送信文 -->
	<stream name="22.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="22.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

</beanio>