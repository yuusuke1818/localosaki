<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

	<!-- *** 日報(取得) テンプレート *** -->
	<!-- ベース部分 12(G2)用コマンド変化に対応 -->
	<template name="base">
		<field name="productCd" length="2" keepPadding="true" />
		<field name="address" length="2" keepPadding="true" />
		<field name="command" length="1" keepPadding="true" />
		<field name="measurePointList" length="2" default="D0" keepPadding="true" />
		<field name="commandParameter" length="2" padding="0" justify="right" />
	</template>

	<!-- ベース部分 共通受信用 -->
	<template name="base.res">
		<field name="productCd" length="2" keepPadding="true" />
		<field name="address" length="2" keepPadding="true" />
		<field name="command" length="3" keepPadding="true" />
		<field name="occursCd" length="2" setter="setOccursCd" />
	</template>

	<!-- 12_HD000用（受信用） -->
	<template name="12HD">
		<field name="measureDayTime" length="10" />
		<segment name="timeZeroList" collection="list" class="map" occursRef="occursCd">
			<field name="demandData" length="4" keepPadding="true" />
		</segment><!-- paramクラスでpointListに追加 -->
		<segment name="timeNumList" collection="list" class="map" occursRef="occursCd">
			<field name="savingDemand" length="4" keepPadding="true" />
		</segment><!-- paramクラスでtimeNumListに追加 -->
		<field name="pointData" length="unbounded" keepPadding="true" setter="setPointData" />
	</template>

	<!-- 「00」(非対応) -->
	<!-- 送信文 -->
	<stream name="00.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="00.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「03」(非対応) -->
	<!-- 送信文 -->
	<stream name="03.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="03.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「06」(非対応) -->
	<!-- 送信文 -->
	<stream name="06.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="06.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「12」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="12.req" format="fixedlength">
		<record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200039Param" />
	</stream>

	<!-- 受信文 -->
	<stream name="12.res" format="fixedlength">
		<record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200039Param">
			<include template="12HD" />
		</record>
	</stream>

	<!-- 「21」非対応 -->
	<!-- 送信文 -->
	<stream name="21.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="21.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「22」非対応 -->
	<!-- 送信文 -->
	<stream name="22.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="22.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

</beanio>