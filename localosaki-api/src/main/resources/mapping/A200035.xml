<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

  <!-- *** イベント制御履歴(FVP)(取得) テンプレート *** -->
  <!-- ベース部分 -->
  <template name="base">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="2" default="QD" keepPadding="true" />
    <field name="loadCtrlAssignment" length="3" padding="0" justify="right" />
  </template>

  <template name="base22">
    <include template="base" />
    <field name="compressionDataLength" length="6" default="000000" keepPadding="true" />
  </template>

  <template name="base.res">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" keepPadding="true" />
  </template>

  <template name="base22.res">
    <include template="base.res" />
    <field name="compressionDataLength" length="6" keepPadding="true" />
  </template>

  <!-- 共通部分１（受信用） -->
  <template name="commonResponse">
    <segment name="eventCtrlHistList" collection="list" class="map" occurs="100">
      <field name="recordDayTimeSeconds" length="12" keepPadding="true" />
      <field name="changefactorEvent" length="1" keepPadding="true" />
      <field name="event1MeasureValue" length="4" keepPadding="true" />
      <field name="event2MeasureValue" length="4" keepPadding="true" />
      <field name="event3MeasureValue" length="4" keepPadding="true" />
    </segment>
  </template>

  <!-- 共通部分２（受信用） -->
  <template name="commonResponse2">
    <segment name="eventCtrlHistEaList" collection="list" class="map" occurs="100">
      <field name="occurDayTimeSeconds" length="12" keepPadding="true" />
      <field name="status" length="1" keepPadding="true" />
      <segment name="eventList" collection="list" class="map" occurs="3">
        <field name="factor" length="1" keepPadding="true" />
        <field name="measureValue" length="5" keepPadding="true" />
      </segment>
    </segment>
  </template>


  <!-- 「00」非対応 -->
  <!-- 送信文 -->
  <stream name="00.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>
  <!-- 受信文 -->
  <stream name="00.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>


  <!-- 「03」非対応 -->
  <!-- 送信文 -->
  <stream name="03.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>
  <!-- 受信文 -->
  <stream name="03.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>


  <!-- 「06」非対応 -->
  <!-- 送信文 -->
  <stream name="06.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>
  <!-- 受信文 -->
  <stream name="06.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>


  <!-- 「12」 製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="12.req" format="fixedlength">
    <record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200035Param" />
  </stream>

  <!-- 受信文 -->
  <stream name="12.res" format="fixedlength">
    <record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200035Param">
      <include template="commonResponse" />
    </record>
  </stream>

  <!-- 「21」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="21.req" format="fixedlength">
    <record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200035Param" />
  </stream>

  <!-- 受信文 -->
  <stream name="21.res" format="fixedlength">
    <record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200035Param">
      <include template="commonResponse2" />
    </record>
  </stream>

  <!-- 「22」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="22.req" format="fixedlength">
    <record name="param" template="base22" class="jp.co.osaki.osol.mng.param.A200035Param" />
  </stream>

  <!-- 受信文 -->
  <stream name="22.res" format="fixedlength">
    <record name="param" template="base22.res" class="jp.co.osaki.osol.mng.param.A200035Param">
      <include template="commonResponse2" />
    </record>
  </stream>

  <!-- 応答データ無し -->
  <stream name="res_data.none" format="fixedlength">
    <record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200035Param">
    </record>
  </stream>

</beanio>