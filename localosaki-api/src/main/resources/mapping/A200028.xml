<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

	<!-- *** 温度制御履歴(取得) テンプレート *** -->
	<!-- ベース部分 -->
	<template name="base">
		<field name="productCd" length="2" keepPadding="true" />
		<field name="address" length="2" keepPadding="true" />
		<field name="command" length="1" default="O" keepPadding="true" />
		<field name="command" length="3" default="000" keepPadding="true" />
		<field name="settingChangeHist" length="1" default="0" keepPadding="true" />
	</template>

	<template name="base.res">
		<field name="productCd" length="2" keepPadding="true" />
		<field name="address" length="2" keepPadding="true" />
		<field name="command" length="5" keepPadding="true" />
	</template>

	<!-- 共通部分１（受信用） -->
	<template name="commonResponse">
		<segment name="loadCtrlHistList" collection="list" class="map" occurs="50">
			<field name="recordDayTimeSeconds" length="12" keepPadding="true" />
			<field name="ctrlTemperature" length="3" keepPadding="true" />
			<field name="ctrlHumidity" length="3" keepPadding="true" />
			<field name="ctrlAlertState" length="2" keepPadding="true" />
			<field name="portOutputState" length="4" keepPadding="true" />
		</segment>
	</template>


	<!-- 「00」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="00.req" format="fixedlength">
		<record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200028Param" />
	</stream>

	<!-- 受信文 -->
	<stream name="00.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200028Param">
			<include template="commonResponse" />
		</record>
	</stream>

	<!-- 「03」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="03.req" format="fixedlength">
		<record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200028Param" />
	</stream>

	<!-- 受信文 -->
	<stream name="03.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200028Param">
			<include template="commonResponse" />
		</record>
	</stream>

	<!-- 「06」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="06.req" format="fixedlength">
		<record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200028Param" />
	</stream>

	<!-- 受信文 -->
	<stream name="06.res" format="fixedlength">
		<record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200028Param">
			<include template="commonResponse" />
		</record>
	</stream>

	<!-- 「12」非対応 -->
	<!-- 送信文 -->
	<stream name="12.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="12.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「21」非対応 -->
	<!-- 送信文 -->
	<stream name="21.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="21.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「22」非対応 -->
	<!-- 送信文 -->
	<stream name="22.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="22.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

</beanio>