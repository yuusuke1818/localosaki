<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

  <!-- *** 常時監視(取得) テンプレート *** -->
  <!-- ベース部分 -->
  <template name="base">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" default="A0000" keepPadding="true" />
  </template>

  <template name="base22">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" default="A0000" keepPadding="true" />
    <field name="compressionDataLength" length="6" default="000000" keepPadding="true" />
  </template>

  <template name="base.res">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" keepPadding="true" />
    <field name="demandInOperation" length="1" keepPadding="true" />
    <field name="demandAlertFlg" length="1" keepPadding="true" />
  </template>

  <template name="base22.res">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" keepPadding="true" />
    <field name="compressionDataLength" length="6" keepPadding="true" />
    <field name="demandInOperation" length="1" keepPadding="true" />
    <field name="demandAlertFlg" length="1" keepPadding="true" />
  </template>

  <!-- 共通部分１（受信用） -->
  <template name="commonResponse">
    <field name="settingDemandChangeFlg" length="1" keepPadding="true" />
    <field name="settingUnitChangeFlg" length="1" keepPadding="true" />
    <field name="transferAlert" length="1" keepPadding="true" />
  </template>

  <!-- 「00」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="00.req" format="fixedlength">
    <record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200031Param" />
  </stream>

  <!-- 受信文 -->
  <stream name="00.res" format="fixedlength">
    <record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200031Param">
      <field name="circuitErrorFlgLON" length="1" keepPadding="true" />
      <include template="commonResponse" />
      <field name="batteryError" length="1" keepPadding="true" />
      <field name="outputTerminalErrorFlg" length="1" keepPadding="true" />
      <segment name="measurePointList" collection="list" class="map" occurs="20">
        <field name="measurePointState" length="1" keepPadding="true" />
      </segment>
    </record>
  </stream>

  <!-- 「03」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="03.req" format="fixedlength">
    <record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200031Param" />
  </stream>

  <!-- 受信文 -->
  <stream name="03.res" format="fixedlength">
    <record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200031Param">
      <field name="circuitErrorFlgRS485" length="1" keepPadding="true" />
      <include template="commonResponse" />
      <field name="batteryErrorCo" length="1" keepPadding="true" />
      <field name="batteryErrorMo" length="1" keepPadding="true" />
      <segment name="measurePointList" collection="list" class="map" occurs="20">
        <field name="measurePointState" length="1" keepPadding="true" />
      </segment>
    </record>
  </stream>

  <!-- 「06」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="06.req" format="fixedlength">
    <record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200031Param" />
  </stream>

  <!-- 受信文 -->
  <stream name="06.res" format="fixedlength">
    <record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200031Param">
      <field name="circuitErrorFlgRS485" length="1" keepPadding="true" />
      <include template="commonResponse" />
      <field name="batteryErrorCo" length="1" keepPadding="true" />
      <field name="batteryErrorMo" length="1" keepPadding="true" />
      <segment name="measurePointList" collection="list" class="map" occurs="20">
        <field name="measurePointState" length="1" keepPadding="true" />
      </segment>
    </record>
  </stream>

  <!-- 「12」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="12.req" format="fixedlength">
    <record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200031Param" />
  </stream>

  <!-- 受信文 -->
  <stream name="12.res" format="fixedlength">
    <record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200031Param">
      <field name="circuitErrorFlgRS485" length="1" keepPadding="true" />
      <include template="commonResponse" />
      <field name="batteryErrorCo" length="1" keepPadding="true" />
      <field name="batteryErrorMo" length="1" keepPadding="true" />
      <segment name="measurePointList" collection="list" class="map" occurs="58">
        <field name="measurePointState" length="1" keepPadding="true" />
      </segment>
    </record>
  </stream>

  <!-- 「21」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="21.req" format="fixedlength">
    <record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200031Param" />
  </stream>

  <!-- 受信文 -->
  <stream name="21.res" format="fixedlength">
    <record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200031Param">
      <field name="circuitErrorFlgRS485" length="1" keepPadding="true" />
      <field name="batteryError" length="1" keepPadding="true" />
      <field name="batteryErrorCo" length="1" keepPadding="true" />
      <field name="batteryErrorMo" length="1" keepPadding="true" />
      <field name="transferAlert" length="1" keepPadding="true" />
      <segment name="measurePointList" collection="list" class="map" occurs="256">
        <field name="measurePointState" length="1" keepPadding="true" />
      </segment>
    </record>
  </stream>

  <!-- 「22」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="22.req" format="fixedlength">
    <record name="request" template="base22" class="jp.co.osaki.osol.mng.param.A200031Param" />
  </stream>

  <!-- 受信文 -->
  <stream name="22.res" format="fixedlength">
    <record name="response" template="base22.res" class="jp.co.osaki.osol.mng.param.A200031Param">
      <field name="circuitErrorFlgRS485" length="1" keepPadding="true" />
      <field name="batteryError" length="1" keepPadding="true" />
      <field name="batteryErrorCo" length="1" keepPadding="true" />
      <field name="batteryErrorMo" length="1" keepPadding="true" />
      <field name="transferAlert" length="1" keepPadding="true" />
      <segment name="measurePointList" collection="list" class="map" occurs="256">
        <field name="measurePointState" length="1" keepPadding="true" />
      </segment>
    </record>
  </stream>

  <!-- 応答データ無し -->
  <stream name="res_data.none" format="fixedlength">
    <record name="response" class="jp.co.osaki.osol.mng.param.A200031Param">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" keepPadding="true" />
    </record>
  </stream>

</beanio>