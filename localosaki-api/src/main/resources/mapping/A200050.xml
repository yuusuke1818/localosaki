<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

  <!-- *** デマンド(設定) Eα テンプレート *** -->
  <!-- ベース部分 -->
  <template name="base">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" default="X0001" keepPadding="true" />
  </template>

  <template name="base22">
    <include template="base" />
    <field name="compressionDataLength" length="6" default="000000" keepPadding="true" />
  </template>

  <template name="base.res">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" keepPadding="true" />
  </template>

  <template name="base22.res">
    <include template="base.res" />
    <field name="compressionDataLength" length="6" keepPadding="true" />
  </template>

  <!-- 共通部分１（送信用） -->
  <template name="commonRequest_1">
    <field name="pulseWeight" length="7" padding="0" justify="right" />
    <field name="targetPower" length="4" padding="0" justify="right" />
    <field name="clockMirrorMethod" length="1" keepPadding="true" />
    <field name="initialPower" length="2" padding="0" justify="right" />
    <field name="highLoadCapacity" length="3" padding="0" justify="right" />
    <field name="limitPower" length="3" padding="0" justify="right" />
    <field name="blockPower" length="4" padding="0" justify="right" />
    <field name="returnPower" length="4" padding="0" justify="right" />
    <field name="alertLockTime" length="2" padding="0" justify="right" />
    <field name="multiBlockTime" length="2" padding="0" justify="right" />
    <segment name="loadInfoList" collection="list" occurs="16" class="map">
      <field name="minLoadBlockTime" length="2" padding="0" justify="right" />
    </segment>
    <field name="blockMethod" length="1" keepPadding="true" />
    <segment name="loadInfoList" collection="list" occurs="16" class="map">
      <field name="loadBlockMethod" length="1" padding="0" justify="right" />
    </segment>
    <segment name="loadInfoList" collection="list" occurs="16" class="map">
      <field name="loadBlockRank" length="3" padding="0" justify="right" />
    </segment>
  </template>

  <!-- 共通部分２（送信用） -->
  <template name="commonRequest_2">
    <segment name="loadInfoList" collection="list" occurs="16" class="map">
      <field name="loadBlockCapacity" length="4" padding="0" justify="right" />
    </segment>
    <field name='switchTarget' length='1' keepPadding="true" />
    <field name='targetPowerMultiplier' length='3' keepPadding="true" />
    <segment name="monthList" collection="list" occurs="12" class="map">
      <field name='maxDemandMonth' length='4' padding="0" justify="right" />
    </segment>
    <segment name="monthList" collection="list" occurs="12" class="map">
      <field name='targetPowerMonth' length='4' padding="0" justify="right" />
    </segment>
  </template>

  <!-- 共通部分３（送信用） -->
  <template name="commonRequest_3">
    <field name='noWeighingDetTime' length='2' padding="0" justify="right" />
    <field name='fuzzy' length='1' keepPadding="true" />
    <field name='switchCoolingAndHeating' length='1' keepPadding="true" />
    <field name='switchCoolingAndHeatingRefPoint' length='3' padding="0" justify="right" />
  </template>


  <!-- 「00」製品に対する設定 -->
  <stream name="00.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="00.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「03」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="03.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="03.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「06」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="06.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="06.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「12」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="12.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="12.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「21」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="21.req" format="fixedlength">
    <record name="response" template="base" class="jp.co.osaki.osol.mng.param.A200050Param">
      <include template="commonRequest_1" />
      <include template="commonRequest_2" />
      <include template="commonRequest_3" />
    </record>
  </stream>
  <!-- 受信文 -->
  <stream name="21.res" format="fixedlength">
    <record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200050Param">
    </record>
  </stream>

  <!-- 「22」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="22.req" format="fixedlength">
    <record name="response" template="base22" class="jp.co.osaki.osol.mng.param.A200050Param">
      <include template="commonRequest_1" />
      <segment name="loadInfoList" collection="list" occurs="16" class="map">
        <field name="temperatureRefPoint" length="3" padding="0" justify="right" />
      </segment>
      <include template="commonRequest_2" />
      <segment name="timeList" collection="list" occurs="48" class="map">
        <field name="targetPower" length="4" padding="0" justify="right" />
      </segment>
      <include template="commonRequest_3" />
    </record>
  </stream>
  <!-- 受信文 -->
  <stream name="22.res" format="fixedlength">
    <record name="response" template="base22.res" class="jp.co.osaki.osol.mng.param.A200050Param">
    </record>
  </stream>

</beanio>