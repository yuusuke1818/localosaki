<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

  <!-- *** イベント制御設定(設定) テンプレート *** -->
  <!-- ベース部分 -->
  <template name="base">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" default="X0045" keepPadding="true" />
  </template>

  <template name="base22">
    <include template="base" />
    <field name="compressionDataLength" length="6" default="000000" keepPadding="true" />
  </template>

  <template name="base.res">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" keepPadding="true" />
  </template>

  <template name="base22.res">
    <include template="base.res" />
    <field name="compressionDataLength" length="6" keepPadding="true" />
  </template>

  <!-- 共通部分2（送信用） -->
  <template name="commonRequest">
    <segment name="loadList" collection="list" class="map" occurs="36">
      <field name="eventCtrlFlg" length="1" keepPadding="true" />
      <segment name="settingEventCtrlList" collection="list" class="map" occurs="3">
        <field name="measuredValue" length="2" keepPadding="true" />
        <field name="ctrlThreshold" length="4" padding="0" justify="right" keepPadding="true"/>
        <field name="ctrlTerms" length="1" keepPadding="true" />
        <field name="eventTerms" length="1" keepPadding="true" />
        <field name="returnWidth" length="3" padding="0" justify="right" />
      </segment>
      <field name="blockHoldTime" length="2" padding="0" justify="right" />
      <field name="returnHoldTime" length="2" padding="0" justify="right" />
    </segment>
  </template>

  <!-- 共通部分（送信用 2） -->
  <template name="commonRequest2">
    <segment name="settingEventCtrlList" collection="list" class="map" occurs="3">
      <field name="measuredTarget" length="1" keepPadding="true" />
      <field name="measuredValue" length="3" keepPadding="true" />
      <field name="ctrlThreshold" length="6" padding="0" justify="right" keepPadding="true"/>
      <field name="ctrlTerms" length="1" keepPadding="true" />
      <field name="eventTerms" length="1" keepPadding="true" />
      <field name="returnWidth" length="5" padding="0" justify="right" />
    </segment>
  </template>

  <!-- 共通部分（送信用 3） -->
  <template name="commonRequest3">
    <segment name="settingEventCtrlList" collection="list" class="map" occurs="3">
      <field name="measuredTarget" length="1" keepPadding="true" />
      <field name="measuredValue" length="3" keepPadding="true" />
      <field name="ctrlThreshold" length="6" padding="0" justify="right" keepPadding="true"/>
      <field name="comparePoint" length="3" padding="0" justify="right" keepPadding="true"/>
      <field name="ctrlTerms" length="1" keepPadding="true" />
      <field name="eventTerms" length="1" keepPadding="true" />
      <field name="returnWidth" length="5" padding="0" justify="right" />
    </segment>
  </template>

  <!-- 共通部分１（受信用） -->
  <template name="commonResponse">
    <field name="settingDate" length="11" keepPadding="true" />
    <segment name="loadList" collection="list" class="map" occurs="36">
      <field name="eventCtrlFlg" length="1" keepPadding="true" />
      <segment name="settingEventCtrlList" collection="list" class="map" occurs="3">
        <field name="measuredValue" length="2" keepPadding="true" />
        <field name="ctrlThreshold" length="4" keepPadding="true" />
        <field name="ctrlTerms" length="1" keepPadding="true" />
        <field name="eventTerms" length="1" keepPadding="true" />
        <field name="returnWidth" length="3" keepPadding="true" />
      </segment>
      <field name="blockHoldTime" length="2" keepPadding="true" />
      <field name="returnHoldTime" length="2" keepPadding="true" />
    </segment>
  </template>


  <!-- 「00」(非対応) -->
  <!-- 送信文 -->
  <stream name="00.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>
  <!-- 受信文 -->
  <stream name="00.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「03」(非対応) -->
  <!-- 送信文 -->
  <stream name="03.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>
  <!-- 受信文 -->
  <stream name="03.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「06」(非対応) -->
  <!-- 送信文 -->
  <stream name="06.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>
  <!-- 受信文 -->
  <stream name="06.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「12」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="12.req" format="fixedlength">
    <record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200021Param">
      <include template="commonRequest" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="12.res" format="fixedlength">
    <record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200021Param">
      <include template="commonResponse" />
    </record>
  </stream>

  <!-- 「21」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="21.req" format="fixedlength">
    <record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200021Param">
      <segment name="loadList" collection="list" class="map" occurs="80">
        <field name="eventCtrlFlg" length="1" keepPadding="true" />
        <include template="commonRequest2" />
        <field name="blockHoldTime" length="2" padding="0" justify="right" />
        <field name="returnHoldTime" length="2" padding="0" justify="right" />
      </segment>
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="21.res" format="fixedlength">
    <record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200021Param">
    </record>
  </stream>

  <!-- 「22」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="22.req" format="fixedlength">
    <record name="param" template="base22" class="jp.co.osaki.osol.mng.param.A200021Param">
      <segment name="loadList" collection="list" class="map" occurs="80">
        <field name="eventCtrlFlg" length="1" keepPadding="true" />
        <include template="commonRequest3" />
        <field name="blockHoldTime" length="2" padding="0" justify="right" />
        <field name="returnHoldTime" length="2" padding="0" justify="right" />
      </segment>
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="22.res" format="fixedlength">
    <record name="param" template="base22.res" class="jp.co.osaki.osol.mng.param.A200021Param">
    </record>
  </stream>

</beanio>