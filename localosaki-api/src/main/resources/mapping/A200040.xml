<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

	<!-- *** 最大デマンド(取得) テンプレート *** -->
	<!-- ベース部分 -->
	<template name="base">
		<field name="productCd" length="2" keepPadding="true" />
		<field name="address" length="2" keepPadding="true" />
		<field name="command" length="2" default="CC" keepPadding="true" />
		<field name="dataHist" length="3" default="000" padding="0" justify="right" />
	</template>
	<template name="base.res">
		<field name="productCd" length="2" keepPadding="true" />
		<field name="address" length="2" keepPadding="true" />
		<field name="command" length="5" keepPadding="true" />
	</template>

	<!-- 共通部分１（受信用） -->
	<template name="commonResponse">
		<field name="recordDayTime" length="10" keepPadding="true" />
		<field name="initialPower" length="2" keepPadding="true" />
		<field name="alertLockTime" length="2" keepPadding="true" />
		<field name="multipleLoadBlTime" length="2" keepPadding="true" />
		<field name="targetPower" length="4" keepPadding="true" />
		<field name="dayMaxDemand" length="4" keepPadding="true" />
		<field name="nonCtrlAvePower" length="5" keepPadding="true" />
		<field name="ctrlBlPower" length="5" keepPadding="true" />
		<field name="plotDataPoint1" length="19" keepPadding="true" />
		<field name="plotDataPoint2" length="19" keepPadding="true" />
		<field name="plotDataPoint3" length="19" keepPadding="true" />
		<field name="plotDataPoint4" length="19" keepPadding="true" />
		<field name="plotDataPoint5" length="19" keepPadding="true" />
		<field name="plotDataPoint6" length="19" keepPadding="true" />
		<field name="plotDataPoint7" length="19" keepPadding="true" />
		<field name="plotDataPoint8" length="19" keepPadding="true" />
		<field name="plotDataPoint9" length="19" keepPadding="true" />
		<field name="plotDataPoint10" length="19" keepPadding="true" />
		<field name="plotDataPoint11" length="19" keepPadding="true" />
		<field name="plotDataPoint12" length="19" keepPadding="true" />
		<field name="plotDataPoint13" length="19" keepPadding="true" />
		<field name="plotDataPoint14" length="19" keepPadding="true" />
		<field name="plotDataPoint15" length="19" keepPadding="true" />
		<field name="plotDataPoint16" length="19" keepPadding="true" />
		<field name="plotDataPoint17" length="19" keepPadding="true" />
		<field name="plotDataPoint18" length="19" keepPadding="true" />
		<field name="plotDataPoint19" length="19" keepPadding="true" />
		<field name="plotDataPoint20" length="19" keepPadding="true" />
		<field name="plotDataPoint21" length="19" keepPadding="true" />
		<field name="plotDataPoint22" length="19" keepPadding="true" />
		<field name="plotDataPoint23" length="19" keepPadding="true" />
		<field name="plotDataPoint24" length="19" keepPadding="true" />
		<field name="plotDataPoint25" length="19" keepPadding="true" />
		<field name="plotDataPoint26" length="19" keepPadding="true" />
		<field name="plotDataPoint27" length="19" keepPadding="true" />
		<field name="plotDataPoint28" length="19" keepPadding="true" />
		<field name="plotDataPoint29" length="19" keepPadding="true" />
		<field name="plotDataPoint30" length="19" keepPadding="true" />
	</template>

	<!-- 「00」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="00.req" format="fixedlength">
		<record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200040Param" />

	</stream>

	<!-- 受信文 -->
	<stream name="00.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200040Param">
			<include template="commonResponse" />
		</record>
	</stream>

	<!-- 「03」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="03.req" format="fixedlength">
		<record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200040Param" />
	</stream>

	<!-- 受信文 -->
	<stream name="03.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200040Param">
			<include template="commonResponse" />
		</record>
	</stream>

	<!-- 「06」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="06.req" format="fixedlength">
		<record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200040Param" />
	</stream>

	<!-- 受信文 -->
	<stream name="06.res" format="fixedlength">
		<record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200040Param">
			<include template="commonResponse" />
		</record>
	</stream>

	<!-- 「12」非対応 -->
	<!-- 送信文 -->
	<stream name="12.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="12.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「21」非対応 -->
	<!-- 送信文 -->
	<stream name="21.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="21.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「22」非対応 -->
	<!-- 送信文 -->
	<stream name="22.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="22.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

</beanio>