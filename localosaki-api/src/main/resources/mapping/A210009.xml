<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

    <!-- *** 需要電力予測データ（取得） テンプレート *** -->
    <!-- ベース部分 -->
    <template name="base">
        <field name="productCd" length="2" keepPadding="true" />
        <field name="address" length="2" keepPadding="true" />
        <field name="command" length="5" default="AM000" keepPadding="true" />
        <field name="selectDate" length="10" keepPadding="true"/>
        <field name="compressionDataLength" length="6" default="000000" keepPadding="true"/>
    </template>

    <template name="base.res">
        <field name="productCd" length="2" keepPadding="true" />
        <field name="address" length="2" keepPadding="true" />
        <field name="command" length="5" keepPadding="true" />
        <field name="selectDate" length="10" keepPadding="true"/>
        <field name="compressionDataLength" length="6" keepPadding="true"/>
    </template>

    <!-- 共通部分（受信用） -->
    <template name="commonResponse">
        <segment name="forecastTimeList" collection="list" occurs="384" class="map">
            <field name="standardDemand" length="4" keepPadding="true"/>
            <field name="comfortableDemand" length="4" keepPadding="true"/>
            <field name="ecoDemand" length="4" keepPadding="true"/>
            <field name="autoDemand" length="4" keepPadding="true"/>
        </segment>
        <segment name="forecastDayList" collection="list" occurs="8" class="map">
            <field name="airDemand" length="4" keepPadding="true"/>
            <field name="totalDemand" length="6" keepPadding="true"/>
        </segment>
    </template>

    <!-- 「00」非対応 -->
    <!-- 送信文 -->
    <stream name="00.req" format="fixedlength">
        <record name="dummy">
            <field name="dummy" length="0" />
        </record>
    </stream>
    <!-- 受信文 -->
    <stream name="00.res" format="fixedlength">
        <record name="dummy">
            <field name="dummy" length="0" />
        </record>
    </stream>

    <!-- 「03」非対応 -->
    <!-- 送信文 -->
    <stream name="03.req" format="fixedlength">
        <record name="dummy">
            <field name="dummy" length="0" />
        </record>
    </stream>
    <!-- 受信文 -->
    <stream name="03.res" format="fixedlength">
        <record name="dummy">
            <field name="dummy" length="0" />
        </record>
    </stream>

    <!-- 「06」非対応 -->
    <!-- 送信文 -->
    <stream name="06.req" format="fixedlength">
        <record name="dummy">
            <field name="dummy" length="0" />
        </record>
    </stream>
    <!-- 受信文 -->
    <stream name="06.res" format="fixedlength">
        <record name="dummy">
            <field name="dummy" length="0" />
        </record>
    </stream>

    <!-- 「12」非対応 -->
    <!-- 送信文 -->
    <stream name="12.req" format="fixedlength">
        <record name="dummy">
            <field name="dummy" length="0" />
        </record>
    </stream>
    <!-- 受信文 -->
    <stream name="12.res" format="fixedlength">
        <record name="dummy">
            <field name="dummy" length="0" />
        </record>
    </stream>

    <!-- 「21」非対応 -->
    <!-- 送信文 -->
    <stream name="21.req" format="fixedlength">
        <record name="dummy">
            <field name="dummy" length="0" />
        </record>
    </stream>
    <!-- 受信文 -->
    <stream name="21.res" format="fixedlength">
        <record name="dummy">
            <field name="dummy" length="0" />
        </record>
    </stream>

    <!-- 「22」製品に対する設定 -->
    <!-- 送信文 -->
    <stream name="22.req" format="fixedlength">
        <record name="param" template="base" class="jp.co.osaki.osol.mng.param.A210009Param">
        </record>
    </stream>
    <!-- 受信文 -->
    <stream name="22.res" format="fixedlength">
        <record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A210009Param">
            <include template="commonResponse" />
        </record>
    </stream>

    <!-- 応答データ無し -->
    <stream name="res_data.none" format="fixedlength">
        <record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A210009Param">
        </record>
    </stream>
</beanio>