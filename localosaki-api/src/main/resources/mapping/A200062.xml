<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

  <!-- *** スケジュールパターン(設定) テンプレート *** -->
  <!-- ベース部分 -->
  <template name="base">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" default="X0002" keepPadding="true" />
  </template>

  <template name="base22">
    <include template="base" />
    <field name="compressionDataLength" length="6" default="000000" keepPadding="true" />
  </template>

  <template name="base.res">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" keepPadding="true" />
  </template>

  <template name="base22.res">
    <include template="base.res" />
    <field name="compressionDataLength" length="6" keepPadding="true" />
  </template>

  <!-- 共通部分１（送信用） -->
  <template name="commonRequest_1">
    <segment name="settingSchedulePatternList" collection="list" class="map" occurs="16">
      <segment name="timeZoneList" collection="list" class="map" occurs="4">
        <field name="startHour" length="2" padding="0" justify="right" />
        <field name="startMinute" length="2" padding="0" justify="right" />
        <field name="endHour" length="2" padding="0" justify="right" />
        <field name="endMinute" length="2" padding="0" justify="right" />
        <field name="onMinute" length="2" padding="0" justify="right" />
        <field name="offMinute" length="2" padding="0" justify="right" />
      </segment>
    </segment>
  </template>


  <!-- 「00」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="00.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="00.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「03」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="03.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="03.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「06」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="06.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="06.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「12」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="12.req" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="12.res" format="fixedlength">
    <record name="dummy">
      <field name="dummy" length="0" />
    </record>
  </stream>

  <!-- 「21」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="21.req" format="fixedlength">
    <record name="response" template="base" class="jp.co.osaki.osol.mng.param.A200062Param">
      <include template="commonRequest_1" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="21.res" format="fixedlength">
    <record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200062Param">
    </record>
  </stream>

  <!-- 「22」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="22.req" format="fixedlength">
    <record name="response" template="base22" class="jp.co.osaki.osol.mng.param.A200062Param">
      <include template="commonRequest_1" />
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="22.res" format="fixedlength">
    <record name="response" template="base22.res" class="jp.co.osaki.osol.mng.param.A200062Param">
    </record>
  </stream>

</beanio>