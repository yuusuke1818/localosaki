<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

	<!-- *** FVx間管理情報(設定) テンプレート *** -->
	<!-- ベース部分 -->
	<template name="base">
		<field name="command" length="2" default="T3" keepPadding="true" />
		<field name="answerCd" length="1" default="0" keepPadding="true" />
	</template>

	<template name="base.res">
		<field name="command" length="2" keepPadding="true" />
		<field name="answerCd" length="1" keepPadding="true" />
	</template>


	<!-- 共通部分１（送信用） -->
	<template name="commonRequest">
		<segment name="setManageInfoFVPx" class="map">
			<field name="numFVPx" length="1" keepPadding="true" />
			<field name="addressFVPx" length="4" padding="0" justify="right" />
			<field name="executePolling" length="1" keepPadding="true" />
			<field name="stateFVPx" length="1" keepPadding="true" />
			<field name="demandAlert" length="1" keepPadding="true" />
			<field name="errorCircuitLON" length="1" keepPadding="true" />
			<field name="settingDemand" length="1" keepPadding="true" />
			<field name="settingUnit" length="1" keepPadding="true" />
			<field name="transferAlert" length="1" keepPadding="true" />
			<field name="batteryErrorAlertFVPx" length="1" keepPadding="true" />
			<field name="reserve" length="1"  keepPadding="true"/>
			<segment name="errorAlertMeasurePointXList" collection="list" class="map" occurs="20">
				<field name="inputON" length="1" keepPadding="true" />
				<field name="inputOFF" length="1" keepPadding="true" />
				<field name="initialState" length="1" keepPadding="true" />
				<field name="noCommunication" length="1" keepPadding="true" />
				<field name="analogMax" length="1" keepPadding="true" />
				<field name="analogMin" length="1" keepPadding="true" />
			</segment>
		</segment>
	</template>

	<!-- 共通部分１（受信用） -->
	<template name="commonResponse">
		<segment name="setManageInfoFVPx" class="map">
			<field name="numFVPx" length="1" keepPadding="true" />
			<field name="addressFVPx" length="4" keepPadding="true" />
			<field name="executePolling" length="1" keepPadding="true" />
			<field name="stateFVPx" length="1" keepPadding="true" />
			<field name="demandAlert" length="1" keepPadding="true" />
			<field name="errorCircuitLON" length="1" keepPadding="true" />
			<field name="settingDemand" length="1" keepPadding="true" />
			<field name="settingUnit" length="1" keepPadding="true" />
			<field name="transferAlert" length="1" keepPadding="true" />
			<field name="batteryErrorAlertFVPx" length="1" keepPadding="true" />
			<field name="reserve" length="1" keepPadding="true" />
			<segment name="errorAlertMeasurePointXList" collection="list" class="map" occurs="20">
				<field name="inputON" length="1" keepPadding="true" />
				<field name="inputOFF" length="1" keepPadding="true" />
				<field name="initialState" length="1" keepPadding="true" />
				<field name="noCommunication" length="1" keepPadding="true" />
				<field name="analogMax" length="1" keepPadding="true" />
				<field name="analogMin" length="1" keepPadding="true" />
			</segment>
		</segment>
	</template>


	<!-- 「00」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="00.req" format="fixedlength">
		<record name="response" template="base" class="jp.co.osaki.osol.mng.param.A200015Param">
			<include template="commonRequest" />
		</record>
	</stream>

	<!-- 受信文 -->
	<stream name="00.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200015Param">
			<include template="commonResponse" />
		</record>
	</stream>

	<!-- 「03」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="03.req" format="fixedlength">
		<record name="response" template="base" class="jp.co.osaki.osol.mng.param.A200015Param">
			<include template="commonRequest" />
		</record>
	</stream>

	<!-- 受信文 -->
	<stream name="03.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200015Param">
			<include template="commonResponse" />
		</record>
	</stream>

	<!-- 「06」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="06.req" format="fixedlength">
		<record name="response" template="base" class="jp.co.osaki.osol.mng.param.A200015Param">
			<include template="commonRequest" />
		</record>
	</stream>

	<!-- 受信文 -->
	<stream name="06.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200015Param">
			<include template="commonResponse" />
		</record>
	</stream>


	<!-- 「12」非対応 -->
	<!-- 送信文 -->
	<stream name="12.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="12.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「21」非対応 -->
	<!-- 送信文 -->
	<stream name="21.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="21.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「22」非対応 -->
	<!-- 送信文 -->
	<stream name="22.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="22.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

</beanio>