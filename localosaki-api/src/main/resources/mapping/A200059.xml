<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

	<!-- *** スケジュール(取得) Eα テンプレート *** -->
	<!-- ベース部分 -->

	<template name="base">
		<field name="productCd" length="2"  keepPadding="true" />
		<field name="address" length="2"  keepPadding="true" />
		<field name="command" length="5" default="V0048" keepPadding="true" />
	</template>

	<template name="base22">
		<include template="base" />
		<field name="compressionDataLength" length="6" default="000000" keepPadding="true" />
	</template>

	<template name="base.res">
		<field name="productCd" length="2" keepPadding="true" />
		<field name="address" length="2" keepPadding="true" />
		<field name="command" length="5" keepPadding="true" />
	</template>

	<template name="base22.res">
		<include template="base.res" />
		<field name="compressionDataLength" length="6" keepPadding="true" />
	</template>

	<!-- 共通部分１（受信用） -->
	<template name="commonResponse1">
		<field name="settingDate" length="10" keepPadding="true" />
		<field name="scheduleControl" length="1" keepPadding="true" />
		<segment name="loadList" collection="list" class="map" occurs="8">
			<segment name="settingMonthScheduleList" collection="list" class="map" occurs="12">
				<field name="sundayPattern" length="2" keepPadding="true" />
				<field name="mondayPattern" length="2" keepPadding="true" />
				<field name="tuesdayPattern" length="2" keepPadding="true" />
				<field name="wednesdayPattern" length="2" keepPadding="true" />
				<field name="thursdayPattern" length="2" keepPadding="true" />
				<field name="fridayPattern" length="2" keepPadding="true" />
				<field name="saturdayPattern" length="2" keepPadding="true" />
				<field name="nationalHolidayPattern" length="2" keepPadding="true" />
				<field name="specificDayAssignment1" length="2" keepPadding="true" />
				<field name="specificDayAssignment2" length="2" keepPadding="true" />
				<field name="specificDayAssignment3" length="2" keepPadding="true" />
				<field name="specificDayAssignment4" length="2" keepPadding="true" />
				<field name="specificDayAssignment5" length="2" keepPadding="true" />
				<field name="specificDayAssignment6" length="2" keepPadding="true" />
				<field name="specificDayAssignment7" length="2" keepPadding="true" />
				<field name="specificDayAssignment8" length="2" keepPadding="true" />
				<field name="specificDayAssignment9" length="2" keepPadding="true" />
				<field name="specificDayAssignment10" length="2" keepPadding="true" />
				<field name="specificDayPattern1" length="2" keepPadding="true" />
				<field name="specificDayPattern2" length="2" keepPadding="true" />
				<field name="specificDayPattern3" length="2" keepPadding="true" />
				<field name="specificDayPattern4" length="2" keepPadding="true" />
				<field name="specificDayPattern5" length="2" keepPadding="true" />
				<field name="specificDayPattern6" length="2" keepPadding="true" />
				<field name="specificDayPattern7" length="2" keepPadding="true" />
				<field name="specificDayPattern8" length="2" keepPadding="true" />
				<field name="specificDayPattern9" length="2" keepPadding="true" />
				<field name="specificDayPattern10" length="2" keepPadding="true" />
			</segment>
		</segment>
	</template>


	<!-- 「00」非対応 -->
	<!-- 送信文 -->
	<stream name="00.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 受信文 -->
	<stream name="00.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「03」非対応 -->
	<!-- 送信文 -->
	<stream name="03.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 受信文 -->
	<stream name="03.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「06」非対応 -->
	<!-- 送信文 -->
	<stream name="06.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 受信文 -->
	<stream name="06.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「12」非対応 -->
	<!-- 送信文 -->
	<stream name="12.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 受信文 -->
	<stream name="12.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「21」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="21.req" format="fixedlength">
		<record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200059Param" />
	</stream>

	<!-- 受信文 -->
	<stream name="21.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200059Param">
			<include template="commonResponse1" />
		</record>
	</stream>

	<!-- 「22」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="22.req" format="fixedlength">
		<record name="request" template="base22" class="jp.co.osaki.osol.mng.param.A200059Param" />
	</stream>

	<!-- 受信文 -->
	<stream name="22.res" format="fixedlength">
		<record name="response" template="base22.res" class="jp.co.osaki.osol.mng.param.A200059Param">
			<include template="commonResponse1" />
		</record>
	</stream>

	<!-- 応答データ無し -->
	<stream name="res_data.none" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200059Param">
		</record>
	</stream>

</beanio>