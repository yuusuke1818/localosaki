<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

  <!-- *** 装置情報(取得) テンプレート *** -->
  <!-- ベース部分 -->
  <template name="base">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" default="V0006" keepPadding="true" />
  </template>

  <template name="base22">
    <include template="base" />
    <field name="compressionDataLength" length="6" default="000000" keepPadding="true" />
  </template>

  <template name="base.res">
    <field name="productCd" length="2" keepPadding="true" />
    <field name="address" length="2" keepPadding="true" />
    <field name="command" length="5" keepPadding="true" />
  </template>

  <template name="base22.res">
    <include template="base.res" />
    <field name="compressionDataLength" length="6" keepPadding="true" />
  </template>

  <!-- 共通部分１（受信用） -->
  <template name="commonResponse">
    <field name="settingDate" length="11" keepPadding="true" />
    <field name="productDiscriminationInfo" length="2" keepPadding="true" />
    <field name="versionInfo" length="3" keepPadding="true" />
    <field name="clockSyncState" length="1" keepPadding="true" />
    <field name="pulseInputMonitor" length="1" keepPadding="true" />
    <field name="errorInfo" length="1" keepPadding="true" />
    <field name="powerFrequency" length="2" keepPadding="true" />
  </template>

  <!-- 共通部分２（受信用） -->
  <template name="commonResponse2">
    <field name="settingDate" length="10" keepPadding="true" />
    <field name="productDiscriminationInfo" length="2" keepPadding="true" />
    <field name="versionInfo" length="3" keepPadding="true" />
    <field name="clockSyncState" length="1" keepPadding="true" />
    <field name="pulseInputMonitor" length="1" keepPadding="true" />
    <field name="errorInfo" length="1" keepPadding="true" />
  </template>

  <!-- 共通部分３（受信用） -->
  <template name="commonResponse3">
    <field name="powerFrequency" length="2" keepPadding="true" />
    <field name="reserve" length="5" keepPadding="true" />
    <segment name="versionWirelessModule" class="map">
      <field name="versionIp" length="5" keepPadding="true" />
      <field name="versionApp" length="3" keepPadding="true" />
    </segment>
  </template>


  <!-- 「00」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="00.req" format="fixedlength">
    <record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200034Param">
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="00.res" format="fixedlength">
    <record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200034Param">
      <include template="commonResponse" />
      <field name="versionLONROM" length="2" keepPadding="true" />
      <field name="reserve" length="1" keepPadding="true" />
    </record>
  </stream>

  <!-- 「03」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="03.req" format="fixedlength">
    <record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200034Param">
    </record>
  </stream>
  <!-- 受信文 -->
  <stream name="03.res" format="fixedlength">
    <record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200034Param">
      <include template="commonResponse" />
      <field name="versionInfoSubROM" length="3" keepPadding="true" />
    </record>
  </stream>

  <!-- 「06」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="06.req" format="fixedlength">
    <record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200034Param">
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="06.res" format="fixedlength">
    <record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200034Param">
      <include template="commonResponse" />
      <field name="versionInfoSubROM" length="3" keepPadding="true" />
      <!-- ※個別の場合は、テンプレート化せずに直接記述することもできる -->
    </record>
  </stream>

  <!-- 「12」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="12.req" format="fixedlength">
    <record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200034Param">
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="12.res" format="fixedlength">
    <record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200034Param">
      <include template="commonResponse" />
      <field name="versionInfoSubROM" length="3" keepPadding="true" />
    </record>
  </stream>

  <!-- 「21」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="21.req" format="fixedlength">
    <record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200034Param">
    </record>
  </stream>

  <!-- 受信文 -->
  <stream name="21.res" format="fixedlength">
    <record name="param" template="base.res" class="jp.co.osaki.osol.mng.param.A200034Param">
      <include template="commonResponse2" />
      <include template="commonResponse3" />
    </record>
  </stream>

  <!-- 「22」製品に対する設定 -->
  <!-- 送信文 -->
  <stream name="22.req" format="fixedlength">
    <record name="param" template="base22" class="jp.co.osaki.osol.mng.param.A200034Param">
    </record>
  </stream>
  <!-- 受信文 -->
  <stream name="22.res" format="fixedlength">
    <record name="param" template="base22.res" class="jp.co.osaki.osol.mng.param.A200034Param">
      <include template="commonResponse2" />
      <field name="errorInfo2" length="1" keepPadding="true" />
      <field name="reserveArea" length="1" keepPadding="true" />
      <include template="commonResponse3" />
      <field name="reserveArea" length="5" keepPadding="true" />
      <field name="reserveArea" length="5" keepPadding="true" />
      <field name="reserveArea" length="5" keepPadding="true" />
    </record>
  </stream>

  <!-- 応答データ無し -->
  <stream name="res_data.none" format="fixedlength">
    <record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200034Param">
    </record>
  </stream>

</beanio>