<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

	<!-- *** スケジュール(取得) テンプレート *** -->
	<!-- ベース部分 -->

	<template name="base">
		<field name="productCd" length="2"  keepPadding="true" />
		<field name="address" length="2"  keepPadding="true" />
		<field name="command" length="1" default="V"  keepPadding="true" />
		<field name="settingChangeHist" length="1" default="0"  keepPadding="true" />
		<field name="command" length="3" default="002"  keepPadding="true" />
	</template>

	<template name="base_12">
		<field name="productCd" length="2"  keepPadding="true" />
		<field name="address" length="2"  keepPadding="true" />
		<field name="command" length="1" default="V"  keepPadding="true" />
		<field name="settingChangeHist" length="1" default="0"  keepPadding="true" />
		<field name="pageAssignment" length="1"  keepPadding="true" />
		<field name="command" length="2" default="02"  keepPadding="true" />
	</template>

	<template name="base.res">
		<field name="productCd" length="2" keepPadding="true" />
		<field name="address" length="2" keepPadding="true" />
		<field name="command" length="5" keepPadding="true" />
	</template>

	<!-- 共通部分1_1 FV2（受信用） -->
	<template name="commonResponse_00">
		<field name="settingDate" length="11" keepPadding="true" />
		<field name="scheduleManageAssignment" length="1" keepPadding="true" />
		<segment name="settingSchedulePatternList" collection="list" class="map" occurs="16">
			<field name="startHour1" length="2" keepPadding="true" />
			<field name="startMinute1" length="2" keepPadding="true" />
			<field name="endHour1" length="2" keepPadding="true" />
			<field name="endMinute1" length="2" keepPadding="true" />
			<field name="startHour2" length="2" keepPadding="true" />
			<field name="startMinute2" length="2" keepPadding="true" />
			<field name="endHour2" length="2" keepPadding="true" />
			<field name="endMinute2" length="2" keepPadding="true" />
			<field name="startHour3" length="2" keepPadding="true" />
			<field name="startMinute3" length="2" keepPadding="true" />
			<field name="endHour3" length="2" keepPadding="true" />
			<field name="endMinute3" length="2" keepPadding="true" />
			<field name="startHour4" length="2" keepPadding="true" />
			<field name="startMinute4" length="2" keepPadding="true" />
			<field name="endHour4" length="2" keepPadding="true" />
			<field name="endMinute4" length="2" keepPadding="true" />
			<field name="dutyOnTime" length="2" keepPadding="true" />
			<field name="dutyOffTime" length="2" keepPadding="true" />
		</segment>
	</template>

	<!-- 共通部分1_2 FVPa(D),FVPa(G2)（受信用） -->
	<template name="commonResponse_0612">
		<field name="settingDate" length="11" keepPadding="true" />
		<field name="scheduleManageAssignment" length="1" keepPadding="true" />
		<segment name="settingSchedulePatternList" collection="list" class="map" occurs="16">
			<field name="startHour1" length="2" />
			<field name="startMinute1" length="2" />
			<field name="endHour1" length="2" />
			<field name="endMinute1" length="2" />
			<field name="startHour2" length="2" />
			<field name="startMinute2" length="2" />
			<field name="endHour2" length="2" />
			<field name="endMinute2" length="2" />
			<field name="startHour3" length="2" />
			<field name="startMinute3" length="2" />
			<field name="endHour3" length="2" />
			<field name="endMinute3" length="2" />
			<field name="startHour4" length="2" />
			<field name="startMinute4" length="2" />
			<field name="endHour4" length="2" />
			<field name="endMinute4" length="2" />
			<field name="dutyOnTime" length="2" />
			<field name="dutyOffTime" length="2" />
			<field name="dutySelect" length="1" />
		</segment>
	</template>

	<!-- 共通スケジュール設定（受信用） -->
	<template name="commonScheduleSet">
		<segment name="settingMonthScheduleList" collection="list" class="map" occurs="12">
			<field name="sundayPattern" length="2" keepPadding="true" />
			<field name="mondayPattern" length="2" keepPadding="true" />
			<field name="tuesdayPattern" length="2" keepPadding="true" />
			<field name="wednesdayPattern" length="2" keepPadding="true" />
			<field name="thursdayPattern" length="2" keepPadding="true" />
			<field name="fridayPattern" length="2" keepPadding="true" />
			<field name="saturdayPattern" length="2" keepPadding="true" />
			<field name="nationalHolidayPattern" length="2" keepPadding="true" />
			<field name="specificDayAssignment1" length="2" keepPadding="true" />
			<field name="specificDayAssignment2" length="2" keepPadding="true" />
			<field name="specificDayAssignment3" length="2" keepPadding="true" />
			<field name="specificDayAssignment4" length="2" keepPadding="true" />
			<field name="specificDayAssignment5" length="2" keepPadding="true" />
			<field name="specificDayAssignment6" length="2" keepPadding="true" />
			<field name="specificDayAssignment7" length="2" keepPadding="true" />
			<field name="specificDayAssignment8" length="2" keepPadding="true" />
			<field name="specificDayAssignment9" length="2" keepPadding="true" />
			<field name="specificDayAssignment10" length="2" keepPadding="true" />
			<field name="specificDayPattern1" length="2" keepPadding="true" />
			<field name="specificDayPattern2" length="2" keepPadding="true" />
			<field name="specificDayPattern3" length="2" keepPadding="true" />
			<field name="specificDayPattern4" length="2" keepPadding="true" />
			<field name="specificDayPattern5" length="2" keepPadding="true" />
			<field name="specificDayPattern6" length="2" keepPadding="true" />
			<field name="specificDayPattern7" length="2" keepPadding="true" />
			<field name="specificDayPattern8" length="2" keepPadding="true" />
			<field name="specificDayPattern9" length="2" keepPadding="true" />
			<field name="specificDayPattern10" length="2" keepPadding="true" />
		</segment>
	</template>

	<!-- 「00」製品に対する設定 -->
	<stream name="00.req" format="fixedlength">
		<record name="request" template="base" class="jp.co.osaki.osol.mng.param.A200004Param" />
	</stream>

	<!-- 受信文 -->
	<stream name="00.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200004Param">
			<include template="commonResponse_00" />
			<segment name="loadList" collection="list" class="map" occurs="6">
				<include template="commonScheduleSet" />
			</segment>
		</record>
	</stream>

	<!-- 「03」非対応 -->
	<!-- 送信文 -->
	<stream name="03.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="03.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「06」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="06.req" format="fixedlength">
		<record name="param" template="base" class="jp.co.osaki.osol.mng.param.A200004Param">
		</record>
	</stream>

	<!-- 受信文 -->
	<stream name="06.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200004Param">
			<include template="commonResponse_0612" />
			<segment name="loadList" collection="list" class="map" occurs="12">
				<include template="commonScheduleSet" />
			</segment>
		</record>
	</stream>

	<!-- 「12」製品に対する設定 -->
	<!-- 送信文 -->
	<stream name="12.req" format="fixedlength">
		<record name="param" template="base_12" class="jp.co.osaki.osol.mng.param.A200004Param">
		</record>
	</stream>

	<!-- 受信文 -->
	<stream name="12.res" format="fixedlength">
		<record name="response" template="base.res" class="jp.co.osaki.osol.mng.param.A200004Param">
			<include template="commonResponse_0612" />
			<segment name="loadList" collection="list" class="map" occurs="12">
				<include template="commonScheduleSet" />
			</segment>
		</record>
	</stream>

	<!-- 「21」非対応 -->
	<!-- 送信文 -->
	<stream name="21.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="21.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

	<!-- 「22」非対応 -->
	<!-- 送信文 -->
	<stream name="22.req" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>
	<!-- 受信文 -->
	<stream name="22.res" format="fixedlength">
		<record name="dummy">
			<field name="dummy" length="0" />
		</record>
	</stream>

</beanio>